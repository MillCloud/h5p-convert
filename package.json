{
    "name": "lumi",
    "version": "0.9.2-beta.1",
    "private": true,
    "description": "A tool to create and display interactive content with H5P",
    "author": "Lumi Education Jan Philip Schellenberg & Sebastian Rettig GbR <c@lumi.education>",
    "main": "build/server/main.js",
    "dependencies": {
        "@lumieducation/h5p-express": "9.1.2",
        "@lumieducation/h5p-html-exporter": "9.1.2",
        "@lumieducation/h5p-server": "9.1.2",
        "@sentry/electron": "3.0.6",
        "@sentry/node": "6.19.6",
        "ajv": "8.11.0",
        "ajv-keywords": "5.1.0",
        "body-parser": "1.20.0",
        "debug": "4.3.4",
        "electron-log": "4.4.6",
        "electron-updater": "5.0.1",
        "express": "4.17.3",
        "express-fileupload": "1.3.1",
        "express-http-proxy": "1.6.3",
        "fs-extra": "10.0.1",
        "i18next": "21.6.15",
        "i18next-http-middleware": "3.2.0",
        "i18next-node-fs-backend": "2.1.3",
        "matomo-tracker": "2.2.4",
        "nanoid": "3.3.2",
        "node-machine-id": "1.1.12",
        "object-hash": "3.0.0",
        "promisepipe": "3.0.0",
        "recursive-readdir": "2.2.2",
        "simple-scorm-packager": "0.2.5",
        "socket.io": "4.4.1",
        "superagent": "7.1.1",
        "tmp-promise": "3.0.3",
        "upath": "2.0.1"
    },
    "scripts": {
        "build:server": "npx tsc --project tsconfig.json",
        "ci": "npm run test",
        "clean": "rm -rf build/",
        "format": "npx prettier --write \"{server,client,test}/**/*.{ts,tsx}\"",
        "format:check": "npx prettier --check \"{server,client,test}/**/*.{ts,tsx}\"",
        "lint": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
        "test:e2e": "npx jest --config jest.e2e.config.js --run-in-band",
        "test:watch": "cross-env NODE_ENV=development jest --watch",
        "test": "cross-env NODE_ENV=development jest && npm run test --prefix client/",
        "semantic-release": "semantic-release",
        "uninstall": "rm -rf build/ && rm -rf node_modules/",
        "install:h5p-core": "sh scripts/install.sh 22115c0aeaa34faef5109c3f378add2d1647c837 c886fa6ded498bbe0148e9484f9b1534facc264e",
        "prepare": "husky install",
        "localize": "json-autotranslate -d --directory-structure ngx-translate -c google_translate.json -s google-translate --decode-escapes -t key-based -i locales/lumi -m i18next && prettier --write \"locales/lumi/*.json\"",
        "start:dev": "cross-env NODE_ENV=development DEBUG=lumi:* LOG_LEVEL=debug PORT=8080 node build/server/main.js",
        "start": "cross-env DEBUG=lumi:* LOG_LEVEL=debug PORT=8080 node build/server/main.js"
    },
    "publishConfig": {
        "access": "restricted"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "@semantic-release/git": "10.0.1",
        "@types/express": "4.17.13",
        "@types/jest": "27.4.1",
        "@types/socket.io": "3.0.1",
        "@types/socket.io-client": "1.4.36",
        "cross-env": "7.0.3",
        "electron": "18.0.3",
        "electron-builder": "23.0.3",
        "electron-notarize": "1.2.1",
        "husky": "7.0.4",
        "jest": "27.5.1",
        "json-autotranslate": "1.10.1",
        "prettier": "2.6.2",
        "semantic-release": "19.0.2",
        "socket.io-client": "4.4.1",
        "spectron": "19.0.0",
        "supertest": "6.2.2",
        "ts-jest": "27.1.4",
        "tslint": "6.1.3",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "tslint-react": "5.0.0",
        "typescript": "4.6.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Lumieducation/Lumi.git"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    }
}
